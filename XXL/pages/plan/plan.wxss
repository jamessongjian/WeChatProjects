  color: #fff;
}

.plan-stats {
  display: flex;
  flex-direction: column;
  align-items: flex-end;
  gap: 8rpx;
}

.plan-time, .plan-score {
  font-size: 24rpx;
  color: rgba(255, 255, 255, 0.9);
}

.plan-summary {
  padding: 16rpx 20rpx;
  background: #f8f9fa;
  display: flex;
  justify-content: space-between;
  border-bottom: 1rpx solid #eee;
}

.summary-item {
  font-size: 24rpx;
  color: #666;
}

/* æ—¶é—´è½´æ ·å¼ */
.timeline {
  padding: 30rpx 20rpx;
  position: relative;
}

.timeline-item {
  position: relative;
  padding-left: 40rpx;
  margin-bottom: 30rpx;
}

.timeline-item:last-child {
  margin-bottom: 0;
}

.timeline-dot {
  position: absolute;
  left: 0;
  top: 10rpx;
  width: 20rpx;
  height: 20rpx;
  border-radius: 50%;
  background: #07C160;
  z-index: 2;
}

.timeline-line {
  position: absolute;
  left: 9rpx;
  top: 30rpx;
  bottom: -30rpx;
  width: 2rpx;
  background: #e0e0e0;
  z-index: 1;
}

.timeline-content {
  position: relative;
}

.time-slot {
  font-size: 26rpx;
  color: #333;
  font-weight: 500;
  margin-bottom: 10rpx;
}

.duration {
  font-size: 24rpx;
  color: #999;
  margin-left: 10rpx;
}

.project-card {
  background: #f9f9f9;
  border-radius: 12rpx;
  padding: 16rpx;
  display: flex;
  align-items: center;
  box-shadow: 0 2rpx 6rpx rgba(0, 0, 0, 0.05);
}

.project-image {
  width: 100rpx;
  height: 100rpx;
  border-radius: 8rpx;
  margin-right: 16rpx;
  background-color: #eee;
}

.project-info {
  flex: 1;
}

.project-name {
  font-size: 28rpx;
  font-weight: 500;
  color: #333;
  margin-bottom: 8rpx;
}

.project-detail {
  display: flex;
  flex-wrap: wrap;
  gap: 12rpx;
}

.queue-time, .rest-time {
  font-size: 24rpx;
  color: #666;
  background: rgba(7, 193, 96, 0.1);
  padding: 4rpx 12rpx;
  border-radius: 30rpx;
}

